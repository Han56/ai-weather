{
  "pages": [
    "pages/index/index",
    "pages/aiDress/aiDress",
    "pages/customLocation/customLocation"
  ],
  "usingComponents": {
    "ec-canvas": "/components/ec-canvas/ec-canvas"
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "requiredPrivateInfos": [
    "getLocation",
    "onLocationChange",
    "startLocationUpdateBackground",
    "chooseAddress"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#f5f5f7",
    "navigationBarTitleText": "天气预报",
    "pageOrientation": "portrait"
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "defaultContentBox": true,
      "tagNameStyleIsolation": "legacy",
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "tabBar": {
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "/image/home/home_unselected.png",
        "selectedIconPath": "/image/home/home_selected.png"
      },
      {
        "pagePath": "pages/aiDress/aiDress",
        "text": "AI穿搭",
        "iconPath": "/image/home/robot_unselected.png",
        "selectedIconPath": "/image/home/robot_selected.png"
      }
    ]
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}